{
    "manifest_version":2,
    "name":"calculator",
    "version":"1.0",
    "description":"Calculator For Arithmetic Process",
    "icons":{
        "128":"./icon/icon128.png",
        "48":"./icon/icon48.png",
        "16":"./icon/icon16.png"
    },
    "browser_action":{
        "default_popup":"index.html",
        "default_icon":"./icon/icon16.png"
    },

    "permissions":[
        "storage"
    ],

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}